import Vue from 'vue';
import Vuex from 'vuex';
import common from './common';{%for item in configs%}
import {{item.name}} from '../view/{{item.name}}/store/index';{%endfor%}

Vue.use(Vuex);

export function createStore () {
  return new Vuex.Store({
    modules: {
      common{%for item in configs%},
      {{item.name}}{%endfor%}
    }
  })
}

